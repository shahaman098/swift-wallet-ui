{
  "name": "@circle-fin/bridge-kit",
  "version": "1.1.1",
  "description": "SDK for seamless cross-chain stablecoin bridging",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "dependencies": {
    "zod": "3.25.67",
    "@circle-fin/provider-cctp-v2": "^1.0.2",
    "abitype": "^1.1.0",
    "@solana/web3.js": "^1.98.4",
    "@ethersproject/address": "^5.8.0",
    "@ethersproject/bytes": "^5.8.0",
    "@ethersproject/units": "^5.8.0",
    "bs58": "6.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.cjs"
    },
    "./chains": {
      "types": "./chains.d.ts",
      "import": "./chains.mjs",
      "require": "./chains.cjs",
      "default": "./chains.cjs"
    }
  },
  "files": [
    "chains.*",
    "index.*",
    "README.md",
    "QUICKSTART.md",
    "CHANGELOG.md",
    "package.json",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "../../dist/kits/bridge-kit"
  }
}